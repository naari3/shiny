                key: "_playIdolSpecialSkillEffect",
                value: function(e, t) {
                    if (ie.includes(t.data.effectType))
                        return Promise.resolve();
                    var n = this.myPlayer
                      , r = this._findActorBy(e);
                    if (t.removalTargetSkillEffects.length > 0) {
                        t.decideRemovalEffectTarget(n, e).removeStatusEffectsWithSkillEffect(t)
                    }
                    switch (t.effectType) {
                    case Q.DAMAGE:
                        return t.data.targetGroup === ne.US ? n.receiveSelfDamage(t.value) : (r && e.actionType !== ee.LINK && (t.value > 0 ? r.decreaseStatusEffectRemainTimes(ae) : 0 === t.value && r.decreaseStatusEffectRemainTimes(se)),
                        this._playJudgeDamageEffect(e, t, !0));
                    case Q.BUFF:
                        return t.data.effectType === $.REGENERATE && t.value > 0 && n.recover(t.value),
                        this._playPlayerBuffEffect(t, n, r);
                    case Q.DEBUFF:
                        return this._playPlayerDebuffEffect(t, n, r);
                    case Q.RECOVER:
                        return this._playPlayerRecoverEffect(t, n, r);
                    default:
                        throw le(t.effectType)
                    }
                }
            }, {
                key: "_playRivalSkillEffect",
                value: function(e, t, n) {
                    if (ie.includes(n.data.effectType))
                        return Promise.resolve();
                    if (n.removalTargetSkillEffects.length > 0) {
                        n.decideRemovalEffectTarget(e, t).removeStatusEffectsWithSkillEffect(n)
                    }
                    var r = this._findActorBy(t);
                    switch (n.effectType) {
                    case Q.DAMAGE:
                        return n.data.targetGroup === ne.US ? e.receiveSelfDamage(n.value) : this._playRivalDamageAppealEffect(n, e, t);
                    case Q.BUFF:
                        return n.data.effectType === $.REGENERATE && n.value > 0 && e.recover(n.value),
                        n.data.effectType === $.RERAISE ? this._playPlayerReraiseEffect(n, e) : this._playPlayerBuffEffect(n, e, r);
                    case Q.DEBUFF:
                        return this._playPlayerDebuffEffect(n, e, r);
                    case Q.RECOVER:
                        return this._playPlayerRecoverEffect(n, e, r);
